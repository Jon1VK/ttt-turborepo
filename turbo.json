{
  "$schema": "https://turbo.build/schema.json",
  "globalDependencies": ["**/.env"],
  "pipeline": {
    "clean": {
      "cache": false
    },
    "//#clean": {
      "cache": false
    },
    "dev": {
      "dependsOn": ["^db:generate"],
      "persistent": true,
      "cache": false
    },
    "type-check": {
      "dependsOn": ["^db:generate"]
    },
    "lint": {
      "dependsOn": ["^db:generate"]
    }
  }
}
